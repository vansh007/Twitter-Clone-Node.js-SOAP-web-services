<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProfileService WSDL service -->

  <service name="Profile" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://vansh"/>
      <parameter name="wsdlServiceElement" value="ProfileService"/>
      <parameter name="schemaQualified" value="http://vansh"/>
      <parameter name="wsdlServicePort" value="Profile"/>
      <parameter name="className" value="vansh.ProfileSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Profile"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getProfile" qname="operNS:getProfile" xmlns:operNS="http://vansh" returnQName="retNS:getProfileReturn" xmlns:retNS="http://vansh" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:uEmail" xmlns:pns="http://vansh" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:fEmail" xmlns:pns="http://vansh" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getFollowers" qname="operNS:getFollowers" xmlns:operNS="http://vansh" returnQName="retNS:getFollowersReturn" xmlns:retNS="http://vansh" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://vansh" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getProfile getFollowers"/>

  </service>
</deployment>
